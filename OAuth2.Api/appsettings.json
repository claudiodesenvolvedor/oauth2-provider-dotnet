{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "MongoDb": {
    "ConnectionString": "mongodb://<host>:<port>",
    "DatabaseName": "<database-name>"
  },
  "Jwt": {
    "Issuer": "https://<issuer>",
    "Audience": "https://<audience>",
    "AccessTokenMinutes": 10,
    "KeyId": "<key-id>"
  },
  "AllowedHosts": "*"
}
